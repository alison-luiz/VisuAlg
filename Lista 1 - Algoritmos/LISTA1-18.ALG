Algoritmo "lista_1_exercicio_18"

Var

   numero, i: real

Inicio

      escreval("Forneça um numero real, informaremos o seu resto ")
      escreval("Digite o numero real ")
      leia(numero)
      
      i <- int(numero)

      escreval("")
      escreval("O valor do resto deste numero é ", numero - i:10:10)



Fimalgoritmo